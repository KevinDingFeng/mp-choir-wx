.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  box-sizing: border-box;
  padding: 0 33rpx;
} 
.navigation{
    margin-top: -50rpx;
    width:100%;
    display: inline-flex;
}
.c_musice{
    height: 100%;
    background-repeat: no-repeat;
    background-size: 100%;
    background-attachment:fixed;
}
.goback{
    width:40rpx;
    height:64rpx;
    overflow:hidden;
    color: #000;
    
    margin-right: 40rpx;
}
.musice_name{
    font-size: 51rpx;
    color: #514098;
    margin-left: 190rpx;
}
.musice_Lyrics{
    width: 100%;
    border-radius: 20rpx;
    background: #FFC04D;
    min-height: 750rpx;
}
.musice_btn{
    width: 100%;
    text-align: center;
    margin-top: 35rpx;
}
.musice_btn>image{
    width: 100rpx;
	height: 150rpx;
}
.bottom-logo{
    width: 100%;
    text-align: right;
    margin-top: 15rpx;
}
.bottom-logo>image {
    width:159rpx;
    height:54rpx;
    display:inline-block;
    overflow:hidden;
}
.Lyrics_text{
    text-align: center;
    margin: 20rpx 0;
    color: #514098;
    font-size: 36rpx;
}
.Lyrics_text1{
    text-align: center;
    margin: 20rpx 0;
    color: #514098;
    font-size: 36rpx;
    font-weight: 700;
}
/* 进度条 */
.middle {
    position: fixed;
    width: 100%;
    top: 294rpx;
    bottom: 300rpx;
    font-size: 0;
}
.progress-wrapper{
    display: -webkit-box;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center;
    width: 80%;
    margin: 0rpx auto;
    padding: 20rpx 0;
}
.progress-wrapper .time.time-l {
    text-align: left;
}
.progress-wrapper .time.time-r {
    text-align: right;
}
.progress-wrapper .time {
  color: #fff;
  font-size: 24rpx;
  -webkit-box-flex: 0;
  -ms-flex: 0 0 60rpx;
  flex: 0 0 60rpx;
  line-height: 60rpx;
  width: 60rpx;
}
.progress-wrapper .progress-bar-wrapper {
    -webkit-box-flex:1;
    -ms-flex:1;
    flex:1;
}
.progress-bar {
    height:30px;
}
progress-bar .bar-inner {
    position:relative;
    top:13px;
    height:4px;
    background:rgba(0,0,0,0.3);
}
.progress-bar .bar-inner .progress {
    position:absolute;
    height:100%;
    background:#000;
}

.text{
    line-height: 32px;
    text-align: center;
    margin: 20rpx 0;
    color: #514098;
    font-size: 28rpx;
    padding:0  28rpx;
}
.current {
    color: #514098;
    font-weight: 700;
}
.middle-r {
    display: inline-block;
    vertical-align: top;
    width: 90%!important;
    height: 100%;
    overflow: hidden;
}
.lyric-wrapper{
    width: 100%;
    margin: 0 auto;
    overflow: hidden;
    text-align: center;
    height: 100%;
}
.progress-bar .bar-inner {
    position: relative;
    top: 13px;
    height: 4px;
    background: #000;
}
.progress-bar .bar-inner .progress {
    position: absolute;
    height: 100%;
    background: #000;
}
.musice_btnlist{
    width: 100%;
    text-align: center;
    margin-top: 35rpx;
    margin-left:-40rpx;
}
.musice_btnlist>image{
    width: 90rpx;
	height: 139rpx;
}
.center_btn{
    margin-left: 70rpx;
    margin-right: 55rpx;
}